# Config Backend
# 'cpu-boost' or 'no-cpu-boost'
_CLOUDRUN_CPU_BOOST=cpu-boost
# 'cpu-throttling' or 'no-cpu-throttling'
_CLOUDRUN_CPU_ALLOCATED=cpu-throttling
